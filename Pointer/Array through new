#include<iostream>
using namespace std;

int main()
{
   // int** x = (int **)malloc(3*sizeof(int));
    int **x = new int*[3];
    for(int i = 0; i < 3; i++){
    x[i] = new int[4];
    for(int j = 0; j < 4; j++){
        cin >> x[i][j];
    }
   }
    for(int i = 0; i < 3; i++){
    
    for(int j = 0; j < 4; j++){
        cout<< x[i][j] <<" ";

    }
    cout<<endl;
   }
   return 0;
}
